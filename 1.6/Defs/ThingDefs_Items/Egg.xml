<Defs>

  <ThingDef ParentName="EggBase">
    <defName>HC_AlligatorturtleEggUnfertilized</defName>
    <label>alligator turtle egg (unfertilized)</label>
    <description>An unfertilized egg laid by an alligator turtle. Has that unique taste of reptilian eggs.</description>
    <statBases>
      <Nutrition>0.25</Nutrition>
      <MarketValue>20</MarketValue>
      <Mass>0.2</Mass>
    </statBases>
    <graphicData>
      <texPath>Things/Item/Resource/AnimalProductRaw/EggRound</texPath>
        <color>(245,245,245)</color>
        <drawSize>1.2</drawSize>
    </graphicData>
    <thingCategories>
      <li>EggsUnfertilized</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>10</daysToRotStart>
      </li>
    </comps>
    <ingestible>
      <preferability>RawBad</preferability>
      <foodType>AnimalProduct</foodType>
      <tasteThought>AteRawFood</tasteThought>
    </ingestible>
  </ThingDef>

